{
  "name": "dev-query",
  "version": "1.0.0",
  "description": "App for searching developers on GitHub.",
  "author": {
    "email": "me@codybrunner.dev",
    "name": "Cody Brunner",
    "url": "https://codybrunner.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rockchalkwushock/dev-query.git"
  },
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "next build",
    "commit": "git cz",
    "dev": "next",
    "format": "prettier --write",
    "lint": "eslint",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "framer-motion": "^4.1.9",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "^10.1.3",
    "next-themes": "^0.0.14",
    "react": "^17.0.2",
    "react-accessible-accordion": "^3.3.4",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-hook-form": "^7.3.6",
    "react-hot-toast": "^1.0.2",
    "react-query": "^3.13.12",
    "sass": "^1.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rockchalkwushock/eslint-config": "^1.0.3",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "graphql-tag": "^2.11.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.5.4",
    "next-sitemap": "^1.6.25",
    "postcss": "^8.2.12",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "@rockchalkwushock/eslint-config",
    "ignorePatterns": [
      "package.json",
      "vercel.json"
    ]
  },
  "lint-staged": {
    "*.+(js|json|md|mdx|ts|tsx|yml|yaml)": [
      "prettier --write"
    ],
    "*.+(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
